{
  "manifest_version": 2,
  "name": "Flash Cards",
  "version": "1.0",
  "permissions": [
    "activeTab",
  	"contextMenus",
  	"tabs",
  	"<all_urls>"
  ],
  "background":{
  	"persistent": false,
  	"scripts":["background.js"]
  },
  "browser_action":{
  	"default_icon": "icon.png",
  	"popup":"background.html"
  },
  "icons":{
  	"16": "icon-Q.png"
  },
  "content_scripts": [
	  {
	    "matches": ["<all_urls>"],
	    "js": ["jquery-3.2.1.js","content.js"]
	  }
  ]
}