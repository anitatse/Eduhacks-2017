{
  "manifest_version": 2,
  "name": "Flash Cards",
  "version": "0.1",
  "permissions": [
  	"contextMenus",
  	"tabs"
  ],
  "background":{
  	"persistent": false,
  	"scripts":["background.js"]
  },
  "icons":{
  	"16": "icon-Q.png"
  },
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["jquery-3.2.1.js","content.js"]
	  }
  ]
}